@charset "utf-8";
/* CSS Document */
body, html, h1, h2, h3, h4, h5, h6, p, ul, ol, li, dt, dl, dd, form {
	margin: 0;
	padding: 0;
	font: 12px "微软雅黑";
}
h1, h2, h3, h4, h5, h6, strong, b {
	font-weight: normal;
}
i, em,b {
	font-style: normal;
}
table {
	border-collapse: collapse;
	border-spacing: 0
}
li {
	list-style: none;
}
a {
	text-decoration: none;
	color:#3e3e3e;
}
a:hover {
	color:#f40;
}
img {
	border: 0;
}
input {
	outline: none;
}
.clearfix:after{
	content:'.';
	display:block;
	overflow:hidden;
    height:0;
	clear: both;
}
